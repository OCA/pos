<odoo>
    <template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/pos_self_service_base/static/src/js/chrome.js"
            />
            <script
                type="text/javascript"
                src="/pos_self_service_base/static/src/js/models.js"
            />
            <script
                type="text/javascript"
                src="/pos_self_service_base/static/src/js/screens.js"
            />
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link
                rel="stylesheet"
                href="/pos_self_service_base/static/src/css/pos_self_service.css"
            />
        </xpath>
    </template>
</odoo>
