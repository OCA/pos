<odoo>



    <record model="ir.ui.view" id="pos_session_product_control_tree_view">
        <field
            name="name"
        >pos.session.product.control.tree (in pos_product_control)</field>
        <field name="model">pos.session.product.control</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id" />
                <field name="product_real_start_value" readonly="1" />
                <field name="product_inventory_start_value" readonly="1" />
                <field name="product_real_end_value" readonly="1" />
                <field name="product_inventory_end_value" readonly="1" />
                <field name="product_real_balance_value" readonly="1" />
                <field name="product_inventory_balance_value" readonly="1" />
            </tree>
        </field>
    </record>

</odoo>
