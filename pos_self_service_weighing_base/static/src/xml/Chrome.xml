<templates id="template" xml:space="preserve">

    <t
        t-name="Chrome"
        t-inherit="point_of_sale.Chrome"
        t-inherit-mode="extension"
        owl="1"
    >
        <xpath expr="//TicketButton" position="attributes">
            <attribute name="t-if">showTicketButton()</attribute>
        </xpath>

        <xpath expr="//HeaderButton" position="attributes">
            <attribute name="t-if">showHeaderButton()</attribute>
        </xpath>
    </t>


</templates>
