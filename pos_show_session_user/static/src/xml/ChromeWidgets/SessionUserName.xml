<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">

    <t t-name="SessionUserName" owl="1">
        <div class="oe_status">
            <span class="username">
                <t t-esc="username" />
            </span>
        </div>
    </t>

    <t
        t-name="Chrome"
        t-inherit="point_of_sale.Chrome"
        t-inherit-mode="extension"
        owl="1"
    >
        <xpath expr="//CashierName" position="before">
            <SessionUserName t-if="env.pos and env.pos.config and !env.isMobile" />
        </xpath>
    </t>


</templates>
