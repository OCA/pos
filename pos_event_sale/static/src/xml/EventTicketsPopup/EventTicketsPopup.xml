<templates id="template" xml:space="preserve">

    <t t-name="EventTicketsPopup" owl="1">
        <div role="dialog" class="modal-dialog">
            <div class="popup popup-text event-tickets-popup">
                <header class="title" t-esc="title" />
                <main>
                    <EventTicketList eventTickets="eventTicketsToDisplay" />
                </main>
                <footer class="footer">
                    <div class="button cancel" t-on-click="cancel">
                        Close
                    </div>
                </footer>
            </div>
        </div>
    </t>

</templates>
