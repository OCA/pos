<odoo>

    <record model="ir.model.access" id="user_combo_rule_access">
        <field name="name">combo.rule user access</field>
        <field name="model_id" ref="model_combo_rule" />
        <field name="group_id" ref="base.group_user" />
        <field name="perm_read" eval="1" />
        <field name="perm_create" eval="1" />
        <field name="perm_write" eval="1" />
        <field name="perm_unlink" eval="1" />
    </record>

    <record model="ir.model.access" id="user_combo_rule_item_access">
        <field name="name">combo.rule.item user access</field>
        <field name="model_id" ref="model_combo_rule_item" />
        <field name="group_id" ref="base.group_user" />
        <field name="perm_read" eval="1" />
        <field name="perm_create" eval="1" />
        <field name="perm_write" eval="1" />
        <field name="perm_unlink" eval="1" />
    </record>

</odoo>
