<odoo>

    <record model="ir.ui.view" id="account_move_form_view">
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <field name="tax_cash_basis_origin_move_id" position="after">
                <field name="splitting_order_id" invisible="not splitting_order_id" />
                <field
                    name="splitting_partner_id"
                    invisible="not splitting_partner_id"
                />
            </field>
        </field>
    </record>



</odoo>
